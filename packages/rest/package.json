{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxojs/rest",
  "version": "0.3.0",
  "description": "Type-Safe Discord REST API Client with Advanced Rate Limiting",
  "keywords": [
    "discord",
    "rest",
    "api",
    "client",
    "rate-limiting",
    "typescript",
    "type-safe",
    "discord-api",
    "bot",
    "discord-bot",
    "http-client",
    "rate-limit",
    "undici",
    "form-data",
    "file-upload",
    "eventemitter",
    "zod",
    "validation",
    "discord-rest"
  ],
  "homepage": "https://github.com/AtsuLeVrai/nyxo.js/tree/main/packages/rest",
  "readme": "README.md",
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AtsuLeVrai/nyxo.js",
    "directory": "packages/rest"
  },
  "bugs": {
    "url": "https://github.com/AtsuLeVrai/nyxo.js/issues"
  },
  "author": {
    "name": "AtsuLeVrai",
    "url": "https://github.com/AtsuLeVrai"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:dev": "rimraf dist && tsc -p tsconfig.json",
    "build:prod": "tsdown",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nyxojs/core": "workspace:^",
    "eventemitter3": "^5.0.1",
    "form-data": "^4.0.4",
    "mime-types": "^3.0.1",
    "undici": "^7.12.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^24.1.0",
    "rimraf": "^6.0.1",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3"
  }
}
